{
  "name": "vue3-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve --port 4601",
    "build": "npx vue-cli-service build",
    "pressr": "cross-env SSR=true npx vue-cli-service build",
    "ssr": "npx nodemon --watch src/components -e js,ts,vue src/server.js",
    "ssr-prod": "node src/server.js",
    "lint": "npx vue-cli-service lint"
  },
  "dependencies": {
    "@solana/web3.js": "^1.31.0",
    "@vue/server-renderer": "^3.0.0-rc.5",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "typescript": "^4.5.4",
    "vue": "^3.0.0-0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "nodemon": "^2.0.15"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
